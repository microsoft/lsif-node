{
  "name": "lsif-util",
  "version": "0.1.1",
  "description": "Utility tools for LSIF development.",
  "main": "./lib/main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/microsoft/lsif-node.git"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "clean": "node ./node_modules/rimraf/bin.js -rf lib",
    "compile": "tsc -p ./",
    "lint": "tslint --fix ./src/*.ts && tslint --fix ./src/test/*.ts",
    "prepublishOnly": "npm install && npm run clean && npm run lint && npm run compile && npm run test"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bin": {
    "lsif-util": "./bin/lsif-util"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.13",
    "@types/node": "^11.11.0",
    "@types/yargs": "^13.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "jsonschema": "^1.2.4",
    "lsif-protocol": "0.3.1",
    "readline": "^1.3.0",
    "typescript-json-schema": "^0.36.0",
    "yargs": "^13.2.4"
  },
  "jest": {
    "testRegex": "lib/test/([^/]+)\\.js$"
  },
  "files": [
    "bin"
  ],
  "keywords": [
    "lsif",
    "utility",
    "tools"
  ]
}
